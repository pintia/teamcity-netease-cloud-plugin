<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>cloud-netease</name>
        <display-name>Netease Cloud Agents</display-name>
        <version>@Version@</version>
        <description>Allow to use Netease Cloud (163yun, 网易蜂巢/网易数帆) containers as build agent.</description>
        <vendor>
            <name>Pintia</name>
            <url>https://www.pat-edu.com</url>
        </vendor>
    </info>
    <requirements min-build="40000" />
    <deployment use-separate-classloader="true" />
</teamcity-plugin>
